          <% if(valErrors.find(err => err.param === 'username')) { 
            const index = valErrors.findIndex(err => err.param === 'username'); %>
            <div class="alert alert-danger"><%= valErrors[index].msg %></div>
          <% } %>
          <div class="form-group required">
            <label for="input-username" class="col-sm-2 control-label">Username</label>
            <div class="col-sm-10">
              <input type="text" class="form-control" id="input-username" placeholder="Username" value="<%= oldInput.username %>" name="username">
