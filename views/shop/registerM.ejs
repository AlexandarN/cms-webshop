<!-- JS SCRIPT to check if Privacy Policy checkbox is checked -->
<script type="text/javascript" language="JavaScript">
  function checkCheckBox(form) {
    if (form.agreeBox.checked == false) {
      alert ('Please read through and agree to the Privacy Policy!');
      return false;
    } else { 	
      return true;
    }
  }
</script> 

