<!-- PARALLAX BANNER -->
<div class="breadcrumb parallax-container">
  <div class="parallax"><img src="/images/prlx.jpg" alt="banner"></div>
  <h1>Register Account</h1>
  <ul>
    <li><a href="/">Home</a></li>
    <!-- <li><a href="#">Account</a></li> -->
    <li><a href="/register">Register</a></li>
  </ul>
</div>
<!-- End of PARALLAX BANNER -->


